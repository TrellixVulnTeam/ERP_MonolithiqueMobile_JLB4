let SignaturePadPage = class SignaturePadPage {
    constructor(modalCtrl) {
        this.modalCtrl = modalCtrl;
        this.signature = '';
    }
    ngOnInit() {
    }
    open() {
        return tslib__WEBPACK_IMPORTED_MODULE_0__["__awaiter"](this, void 0, void 0, function* () {
            const modal = yield this.modalCtrl.create({
                component: src_app_modals_signature_picker_signature_picker_page__WEBPACK_IMPORTED_MODULE_3__["SignaturePickerPage"],
                showBackdrop: true,
            });
            modal.onDidDismiss().then(signature => {
                let signatures = localStorage.getItem('signature');
                if (signatures && signatures != null && signatures != 'null') {
                    this.signature = signatures;
                }
            });
            yield modal.present();
        });
    }
