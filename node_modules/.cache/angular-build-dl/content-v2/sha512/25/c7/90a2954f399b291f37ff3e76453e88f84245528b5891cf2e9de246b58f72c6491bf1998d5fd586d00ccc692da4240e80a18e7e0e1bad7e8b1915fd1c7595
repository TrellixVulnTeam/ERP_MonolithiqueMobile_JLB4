let OfflineCreatePage = class OfflineCreatePage {
    constructor(util, navCtrl, sqlite, api) {
        this.util = util;
        this.navCtrl = navCtrl;
        this.sqlite = sqlite;
        this.api = api;
        this.produits_commandes_list = [];
        if (localStorage.getItem("produits_commandes") != null) {
            this.produits_commandes_list = JSON.parse(localStorage.getItem("produits_commandes"));
        }
    }
    ngOnInit() {
        this.badgecount = 0;
        this.isCreate = false;
    }
    addList() {
        /*  if (this.title == '') {
            this.util.showWarningAlert('Please Add Title');
            return false;
          }
          if (this.description === '') {
            this.util.showWarningAlert('Please add Description');
            return false;
          }
          this.isCreate = true;
          this.sqlite.create({
            name: 'offline5.db',
            location: 'default'
          }).then((db: SQLiteObject) => {
