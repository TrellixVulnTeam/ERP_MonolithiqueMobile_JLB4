            console.log(e);
            this.util.showToast('Error', 'danger', 'bottom');
          });*/
    }
    charge_compteur() {
        if (localStorage.getItem("compteur") != null) {
            this.badgecount = localStorage.getItem("compteur");
        }
        else {
            this.badgecount = 0;
        }
        return this.badgecount;
    }
    addtocart() {
        this.navCtrl.navigateRoot(['my-cart']);
    }
    adduser(nom, prenom, adrmail, nomutilid, motdepasse, adr, ville, datenaiss, tel) {
        alert(nomutilid);
        this.api.get('user/findUserByUserName/' + nomutilid).then((data) => {
            const info = JSON.parse(data.data);
            if (info.length > 0) {
                this.util.showToast('Utilisateur existe déjà', 'danger', 'bottom');
            }
            else {
                this.api.get('user/findUserByUserName/' + nomutilid).then((data) => {
                }).catch(error => {
                    //this.isLogin = false;
                    alert('getallproducts');
                    alert(error.message);
                    //this.util.showToast(`${error}`, 'danger', 'bottom');
                });
            }
        }).catch(error => {
            //this.isLogin = false;
            alert('getallproducts');
            alert(error.message);
            //this.util.showToast(`${error}`, 'danger', 'bottom');
        });
    }
};
