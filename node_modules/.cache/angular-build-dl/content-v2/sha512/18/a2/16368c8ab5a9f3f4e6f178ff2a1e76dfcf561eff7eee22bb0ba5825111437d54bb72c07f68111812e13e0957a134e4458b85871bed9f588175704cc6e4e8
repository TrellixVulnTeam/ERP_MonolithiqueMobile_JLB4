= new google.maps.Map(this.mapElement.nativeElement, mapOptions);\n    const marker = new google.maps.Marker({\n      map: this.map,\n      position: latLng\n    });\n    const sunCircle = {\n      strokeColor: \"#49befc\",\n      strokeOpacity: 0.8,\n      strokeWeight: 2,\n      fillColor: \"#49befc\",\n      fillOpacity: 0.35,\n      map: this.map,\n      center: latLng,\n      radius: this.radius * 1000 // in meters\n    };\n    this.circle = new google.maps.Circle(sunCircle);\n    this.circle.bindTo('center', marker, 'position');\n\n  }\n  changeMap() {\n    console.log('radius', this.radius);\n    this.circle.setRadius(this.radius * 1000);\n  }\n\n  updateIt() {\n    console.log('uodate it');\n  }\n\n}\n"],"sourceRoot":"webpack:///","file":"pages-maps-styled-styled-module-es5.js"}