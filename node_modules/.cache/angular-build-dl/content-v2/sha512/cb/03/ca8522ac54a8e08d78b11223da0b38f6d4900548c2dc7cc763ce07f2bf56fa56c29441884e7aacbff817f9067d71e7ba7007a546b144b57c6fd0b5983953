uid')\n    }\n    this.api.post(param, 'ImagesCRUD/getById').then((data) => {\n      const info = JSON.parse(data.data);\n      console.log('data', info);\n      if (info && info.status === 200) {\n        this.lists = [];\n        this.lists = info.data;\n        if (this.lists.length > 0) {\n          this.haveData = true;\n        }\n      }\n    }).catch(error => {\n      console.log('errr', error);\n      this.util.showToast('something went wrong', 'danger', 'bottom');\n    });\n  }\n\n  ngOnInit() {\n\n  }\n  addNew() {\n    this.router.navigate(['rest-image-create']);\n  }\n  getImage(img) {\n    return this.api.imagePath + img;\n  }\n  async deleteItem(item) {\n    const alert = await this.alertCtrl.create({\n      header: 'are you sure?',\n      message: 'to delete this item',\n      buttons: [\n        {\n          text: 'No',\n          role: 'cancel',\n          cssClass: 'secondary',\n          handler: (blah) => {\n            console.log('Confirm Cancel: blah');\n          }\n        }, {\n          text: 'Yes',\n          handler: () => {\n            console.log('Confirm Okay');\n            this.util.show();\n            const param = {\n              id: item\n            }\n            this.api.post(param, 'ImagesCRUD/deleteList').then((data) => {\n              this.util.hide();\n              const info = JSON.parse(data.data);\n              console.log(info);\n              this.getItems();\n            }).catch(error => {\n              this.util.hide();\n              console.log(error);\n              this.util.showToast('Something went wrong', 'danger', 'bottom');\n            });\n          }\n        }\n      ]\n    });\n\n    await alert.present();\n  }\n  async launchMenu(item) {\n    const actionSheet = await this.actionSheetController.create({\n      header: 'Action',\n      buttons: [\n        {\n          text: 'Edit',\n          icon: 'create',\n          handler: () => {\n            console.log('Edit clicked');\n            con